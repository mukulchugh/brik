{
  "name": "@brik/compiler",
  "version": "0.3.0",
  "description": "JSX/TSX compiler for Brik - transforms React components to native code",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "compiler",
    "jsx",
    "tsx",
    "react",
    "swiftui",
    "jetpack-compose",
    "transpiler",
    "code-generation"
  ],
  "author": "Mukul Chugh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mukulchugh/brik.git",
    "directory": "packages/brik-compiler"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/core": "^7.24.7",
    "@babel/parser": "^7.24.7",
    "@babel/traverse": "^7.24.7",
    "@babel/types": "^7.24.7",
    "@brik/core": "workspace:*",
    "@brik/schemas": "workspace:*",
    "@brik/target-compose": "workspace:*",
    "@brik/target-swiftui": "workspace:*"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^20.14.11",
    "typescript": "^5.5.4"
  }
}
