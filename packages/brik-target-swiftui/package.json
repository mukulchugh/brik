{
  "name": "@brik/target-swiftui",
  "version": "0.3.0",
  "description": "SwiftUI code generation target for Brik compiler",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "swiftui",
    "ios",
    "widgets",
    "widgetkit",
    "activitykit",
    "live-activities",
    "code-generation",
    "compiler-target"
  ],
  "author": "Mukul Chugh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mukulchugh/brik.git",
    "directory": "packages/brik-target-swiftui"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@brik/core": "workspace:*",
    "@brik/schemas": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "typescript": "^5.5.4"
  }
}
